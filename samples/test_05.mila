program test_05_int_float;

var i: Integer;
var f: Float;
const a = 3;
const b = 1.234;

begin
    i := 2;
    f := 2.5;
    writeln(i);
    writeln(f);
    
    f := i;
    writeln(f);
    
    f := f + 3.14159;
    i := f;
    writeln(i);

    writeln(123 * 1.0);
    
    writeln(a);
    writeln(b);
    writeln(to_float(a));
    writeln(to_integer(b));
    
    exit
end.